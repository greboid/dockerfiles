{{ with $.volumes }}
{{ $index := 0 -}}
RUN set -eux; \
    #Create required directories
    mkdir -p \
        {{range $key, $value := $.volumes -}}
        {{- $index = increment_int $index -}}
        /rootfs{{$value}} {{- if ne $index (len $.volumes) -}} \{{ end }}
        {{ end -}}
{{ end -}}
